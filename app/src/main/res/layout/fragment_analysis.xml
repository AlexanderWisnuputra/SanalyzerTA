        android:textSize="35sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/StockName22" />
    <TextView
        android:id="@+id/industri3"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
